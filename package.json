    "ms": "^2.1.3",
    "node-pg-migrate": "^7.8.0",
    "pg": "^8.13.1",
    "spotify-web-api-node": "^5.0.2",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/ms": "^0.7.34",
    "@types/node": "^20.11.0",
    "@types/pg": "^8.11.10",
    "@types/spotify-api": "^0.0.25",
    "@types/spotify-web-api-node": "^5.0.11",
